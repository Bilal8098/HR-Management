        </TableView>
        <Button fx:id="backButton" layoutX="480.0" layoutY="347.0" prefHeight="31.0" prefWidth="75.0" style="-fx-background-color: gold;" text="Back" onAction="#handleBackButtonAction">
            <font>
                <Font name="System Bold Italic" size="16.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
