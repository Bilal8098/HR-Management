<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.hrmanagementemployee.ViewMessage">
    <children>
        <Text fill="#ffc605" layoutX="28.0" layoutY="29.0" style="-fx-font-weight: bold;" text="Employee Instance">
            <font>
                <Font size="17.0" />
            </font>
        </Text>
        <Text fill="WHITE" layoutX="225.0" layoutY="71.0" text="Message View">
            <font>
                <Font name="System Bold Italic" size="23.0" />
            </font>
        </Text>
        <TableView fx:id="messageTable" layoutX="31.0" layoutY="101.0" prefHeight="236.0" prefWidth="553.0">
            <columns>
                <TableColumn fx:id="messageIdColumn" prefWidth="91.0" text="Message ID" />
                <TableColumn fx:id="messageTextColumn" prefWidth="267.0" text="Message Text" />
                <TableColumn fx:id="createdAtColumn" prefWidth="150.0" text="Created At" />
